{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-todo/build/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "NeuTodo",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/react-todo/public/logo192.png"
    }
  },
  "cli": {
    "binaryName": "todo-cra-with-neutralino",
    "resourcesPath": "/react-todo/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/react-todo/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}